---
import Hero from '$components/Hero.astro';
import Hero2 from '$components/Hero2.astro';
import Hero3 from '$components/Hero3.astro';
import Card from '$components/Card.astro';
import Feature from '$components/Feature.astro';
import Layout from '../layouts/Page.astro';

const seo = {
  title: 'Astro Minimal Starter',
  description:
    'This is a starter repo for an blog built with Astro including open graph tags, canonical urls, RSS, sitemap support.',
};
---

<Layout seo={seo}>
  <Hero2 />
  <!-- <Hero /> -->
  <Feature />
  <Hero3 />
</Layout>

