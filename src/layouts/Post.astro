---
import Layout from './Page.astro';
import type { SeoProps } from '../components/Astro/BaseHead.astro';
import ArticleSchema from '../components/Astro/ArticleSchema.astro';

export interface Props {
  seo?: SeoProps;
}

const { seo } = Astro.props as Props;
---

<Layout seo={seo}>
  <Fragment>
    <ArticleSchema {...seo} />
  </Fragment>
  <slot />
</Layout>
