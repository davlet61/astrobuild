    "@radix-ui/react-slot": "^1.1.0",
    "@rive-app/canvas": "^2.15.6",
    "@rive-app/react-canvas": "^4.14.5",
    "astro": "4.16.6",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
