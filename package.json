  },
  "scripts": {
    "dev:ssl": "astro dev --port 5001 & caddy run",
    "dev": "astro dev",
    "dev:prod": "TINA_PUBLIC_IS_LOCAL=true tinacms dev -c \"astro dev --port 5001\"",
