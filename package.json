    "@changesets/cli": "^2.27.1",
    "@googlemaps/js-api-loader": "^1.16.6",
    "@hookform/resolvers": "^3.3.4",
    "@keystatic/astro": "^5.0.0",
    "@keystatic/core": "^0.5.17",
    "@radix-ui/react-accordion": "^1.2.1",
